{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "outDir": "./dist",
    "lib": [
      "es6"
    ],
    "declaration": true,
    "declarationMap": true,
    "target": "ES2021",
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "tests"
  ]
}
